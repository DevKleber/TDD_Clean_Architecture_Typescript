{
  "compilerOptions": {
	"outDir": "dist",
	"rootDir": "src",
	"skipLibCheck": true,
	"sourceMap": true,
	"forceConsistentCasingInFileNames": true,
	"target": "ES2022",
	"module": "commonjs",
	"moduleResolution": "node",
	"esModuleInterop": true,
	"baseUrl": "src",
	"paths": {
		"@/*":["*"],
	},
	"strict": true,
  },
  "include": ["src"]
}
